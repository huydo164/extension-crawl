{
  "name": "CrawlImage",
  "description": "Crawling image amazon",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon-crawl.png",
    "32": "icon-crawl.png",
    "48": "icon-crawl.png",
    "128": "icon-crawl.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/-/en/*/dp/*"
      ],
      "js": [
        "/js/app.js"
      ]
    }
  ]
}